{
  "name": "flare-core",
  "version": "0.2.1",
  "description": "Applicative-style UIs",
  "main": "esm/index.js",
  "module": "esm/index.js",
  "unpkg": "umd/flare-core.min.js",
  "typings": "types/index.d.ts",
  "style": "css/flare-core.css",
  "scripts": {
    "docs": "typedoc src/index.tsx && replace '\\(README\\.md#' '(#' docs/README.md",
    "precommit": "lint-staged",
    "prepublishOnly": "node -e \"['cjs', 'css', 'esm', 'types'].forEach(x => {try{require('fs').rmSync(x, { recursive: true })}catch(_){}})\" && node css && tsc --project tsconfig.types.json && tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json && rollup -c",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsaunders/flare.git"
  },
  "keywords": [
    "flare",
    "applicative",
    "ui",
    "fp",
    "functional",
    "knobs",
    "controls"
  ],
  "author": "Nick Saunders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nsaunders/flare/issues"
  },
  "homepage": "https://github.com/nsaunders/flare#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.30.0",
    "eslint-config-flare-prettier": "workspace:*",
    "eslint-config-flare-react": "workspace:*",
    "eslint-config-flare-react-typescript": "workspace:*",
    "eslint-config-flare-typescript": "workspace:*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fp-ts": "^2.13.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-config-flare": "workspace:*",
    "react-dom": "^18.2.0",
    "replace": "^1.2.2",
    "rollup": "^3.9.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
