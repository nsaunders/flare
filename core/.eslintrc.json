{
  "root": true,
  "extends": [
    "eslint:recommended",
    "flare-react",
    "flare-typescript",
    "flare-react-typescript",
    "flare-prettier"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "overrides": [
    {
      "files": "*.config.js",
      "env": {
        "browser": false,
        "node": true
      }
    }
  ]
}
